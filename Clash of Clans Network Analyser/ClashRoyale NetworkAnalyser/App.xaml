             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:ClashRoyale_NetworkAnalyser"
             StartupUri="MainWindow.xaml" Exit="Application_Exit"
             ShutdownMode="OnExplicitShutdown">
